
fn fib_rec(x: i32) -> i32 {
    if x <= 1 {
        return x;
    }
    return fib_rec(x - 1) + fib_rec(x - 2);
}

fn compute_stuff(n: i32) -> i32 {
    let mut x: i32 = 0;
    while x++ < n {}

    return x - 1011;
}

fn main(argc: i32, argv: *const *const char) -> i32 {
    let x: i32 = compute_stuff(1024);
    if fib_rec(x) != 233 {
        return 0;
    }
    return -1;
}
